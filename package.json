{
  "name": "mealmate-web",
  "description": "Application web complète de gestion du diabète avec IA, interfaces spécialisées pour patients, médecins et administrateurs",
  "keywords": [
    "diabetes",
    "healthcare",
    "medical",
    "nutrition",
    "ai",
    "react",
    "typescript",
    "tailwindcss",
    "gemini-ai",
    "cameroon",
    "diabète",
    "santé",
    "nutrition",
    "médical"
  ],
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "node start-dev.js",
    "dev:vite": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "db": "node simple-server.cjs",
    "dev:full": "concurrently \"npm run dev:vite\" \"npm run db\"",
    "clean": "rm -rf dist node_modules/.vite",
    "reset": "npm run clean && npm install",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "express": "^5.1.0",
    "globals": "^16.4.0",
    "json-server": "^1.0.0-beta.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
